!!fizzbuzz | i32 -> i32 {
	!clone !print
	": " !print
	3 !clone_over !mod 0 !eq if {
		"fizz" !print
	}
	5 !clone_over !mod 0 !eq if {
		"buzz" !print
	}
	!println
}

!!while_check | i32 i32 -> i32 i32 bool {
	!clone_over !clone_over
	!gt !not
}

100
0

!while_check while {

	1 !add
	!fizzbuzz
	!while_check
}
